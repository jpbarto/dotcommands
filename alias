alias _docker_run='docker run -ti --rm -v $HOME/.vimrc:/root/.vimrc'
alias _docker_run_aws='_docker_run -v $PWD:/root/dev:rw -v $HOME/.aws:/root/.aws'
alias aws-shell='_docker_run_aws jpbarto/aws-shell:dotcommand'
alias boto3='_docker_run_aws -v $PWD:/root/dev:rw jpbarto/boto3:dotcommand'
alias eb='_docker_run_aws  jpbarto/eb:dotcommand'
alias etcdctl='_docker_run jpbarto/etcdctl:dotcommand'
alias ghci='_docker_run jpbarto/ghci:dotcommand'
alias golang='_docker_run -v $HOME:/root -v $PWD:/root/dev:rw jpbarto/golang:dotcommand'
alias jekyll='_docker_run -v $PWD:/srv/jekyll -p 4000:4000  jpbarto/jekyll:dotcommand'
alias jupyter='_docker_run -p 8888:8888 -v $PWD:/root/dev:rw jpbarto/jupyter:dotcommand'
alias mysql='_docker_run  jpbarto/mysql:dotcommand'
alias packer='_docker_run_Aws  jpbarto/packer:dotcommand'
alias psql='_docker_run jpbarto/psql:dotcommand'
alias redis-cli='_docker_run jpbarto/redis-cli:dotcommand'
alias terraform='_docker_run -v $PWD:/root/dev:rw  jpbarto/terraform:dotcommand'
alias ubuntu='_docker_run -v $PWD:/root/dev  jpbarto/ubuntu:dotcommand'
alias vault='_docker_run jpbarto/vault:dotcommand'
